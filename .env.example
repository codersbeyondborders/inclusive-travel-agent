# Choose Model Backend: 0 -> ML Dev, 1 -> Vertex
# Using ML Dev for accessible travel agent (no Vertex AI dependency)
GOOGLE_GENAI_USE_VERTEXAI=0
# ML Dev backend config - Required for this deployment
GOOGLE_API_KEY=YOUR_GOOGLE_AI_API_KEY_HERE

# Cloud Run deployment config (replaces Vertex AI)
GOOGLE_CLOUD_PROJECT=YOUR_CLOUD_PROJECT_ID_HERE
GOOGLE_CLOUD_REGION=us-central1

# Places API
GOOGLE_PLACES_API_KEY=YOUR_API_KEY_HERE

# Optional: GCS Storage Bucket for file storage
GOOGLE_CLOUD_STORAGE_BUCKET=YOUR_BUCKET_NAME_HERE

# Accessibility API Keys (add as needed)
WHEELMAP_API_KEY=YOUR_WHEELMAP_API_KEY_HERE
ACCESSIBLEGO_API_KEY=YOUR_ACCESSIBLEGO_API_KEY_HERE

# Email Service Configuration (for notifications and confirmations)
EMAIL_ADDRESS=your_email@gmail.com
EMAIL_PASSWORD=your_app_password
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
EMAIL_USE_TLS=true

# Sample Scenario Path - Default is an accessible user profile
# This will be loaded upon first user interaction.
#
# Choose one of the profiles below, or create your own:
#
# INCLUSIVE_TRAVEL_AGENT_SCENARIO=inclusive_travel_agent/profiles/itinerary_seattle_example.json
# INCLUSIVE_TRAVEL_AGENT_SCENARIO=inclusive_travel_agent/profiles/itinerary_empty_default.json
INCLUSIVE_TRAVEL_AGENT_SCENARIO=inclusive_travel_agent/profiles/itinerary_accessible_default.json
